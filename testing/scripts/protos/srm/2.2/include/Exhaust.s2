DEFUN ExhString i var val : var_r
   TEST ${i} == 0
      ASSIGN var_r "${var}=${val} "
   || TEST ${i} == 1
      ASSIGN var_r "${var}=\"\" "
   || ASSIGN var_r ""

DEFUN ExhStringArr i var val : var_r
   TEST ${i} == 0
      ASSIGN var_r "${var}[${val}] "
   || TEST ${i} == 1
      ASSIGN var_r "${var}[] "
   || ASSIGN var_r ""

