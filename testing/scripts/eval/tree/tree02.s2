% Tree evaluation test

NOP 0
&& SYSTEM echo "&& 2a: ${?}/${!}" && exit 2
|| SYSTEM echo "|| 2b: ${?}/${!}" && exit 2
&& SYSTEM echo "&& 0a: ${?}/${!} (never reached)"
&& SYSTEM echo "&& 0b: ${?}/${!} (never reached)"
&& SYSTEM echo "&& 0c: ${?}/${!} (never reached)"
|| SYSTEM echo "|| 0d: ${?}/${!}"
&& SYSTEM echo "&& 0e: ${?}/${!}"
