; CMP operations test

NOP 0
&& TEST 0 == 0
   SYSTEM echo EQ1
&& TEST "abcd" == "abcd"
   SYSTEM echo EQ2
&& TEST 1 != 0
   SYSTEM echo NE1
&& TEST abcd != dcba
   SYSTEM echo NE2
&& TEST 0 < 1
   SYSTEM echo LT1
&& TEST abcd < dcba
   SYSTEM echo LT2
&& TEST 1 > 0
   SYSTEM echo GT1
&& TEST dcba > abcd 
   SYSTEM echo GT2
&& TEST 0 <= 1
   SYSTEM echo LE1
&& TEST abcd <= dcba
   SYSTEM echo LE2
&& TEST 1 >= 0
   SYSTEM echo GE1
&& TEST dcba >= abcd 
   SYSTEM echo GE2
