% srmGetRequestTokens

srmGetRequestTokens $ENV{ENDPOINT} authorizationID=$ENV{USER} userRequestDescription=<userRequestDescription> requestTokens=(?P<requestTokens>.*) returnStatus.explanation=(?P<returnStatus_explanation>.*) returnStatus.statusCode=(SRM_SUCCESS|SRM_DONE|SRM_REQUEST_QUEUED|SRM_REQUEST_INPROGRESS)
