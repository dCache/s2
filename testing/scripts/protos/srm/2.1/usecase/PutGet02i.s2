; PutGet02: transfer of two files in one go to/from SRM server
;
; srmPrepareToGet
; srmPrepareToPut
; srmPutDone
; srmStatusOfGetRequest
; srmStatusOfPutRequest

ASSIGN srmTest PutGet02
   #include ../include/PutCycle.s2
   #include ../include/GetCycle.s2

&& FUN PutCycle "$ENV{FILE_TO_PUT0} $ENV{FILE_TO_PUT1}" "$ENV{NEW_SRM_FILE0} $ENV{NEW_SRM_FILE1}"
&& FUN GetCycle "$ENV{FILE_TO_GET0} $ENV{FILE_TO_GET1}" "$ENV{NEW_SRM_FILE0} $ENV{NEW_SRM_FILE1}"

&& SYSTEM echo "${srmTest}: OK" >> $ENV{SRM2_SUPPORTED}
|| SYSTEM echo "${srmTest}: KO ${-returnStatus_explanation}" >> $ENV{SRM2_SUPPORTED}
