TEST $MATCH{(?P<srmTest>.*).s2$ ${0}}
   #include ../include/Exhaust.s2

DEFUN srmTest authorizationID spaceTokens
   >0&&2 FUN ExhString $I{0} authorizationID ${authorizationID} : arg0
      >0&&2 FUN ExhStringArr $I{0} spaceTokens ${spaceTokens} : arg1
         ASSIGN c $EXPR{${c}+1}
         && ASSIGN script "eval=2 srmGetSpaceMetaData $ENV{ENDPOINT} ${arg0}${arg1}spaceDetails=(?P<spaceDetails>.*) returnStatus.explanation=(?P<returnStatus_explanation>.*) returnStatus.statusCode=(SRM_SUCCESS|SRM_DONE|SRM_SPACE_AVAILABLE)"
         && FUN S2 ${script} $PRINTF{%02d ${c}}
         % Ignore errors
         || NOP

%%% main() %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
&& ASSIGN c 0
%&& <CODE FROM BASIC>
&& ASSIGN spaceTokens spaceTokens
&& FUN srmTest $ENV{USER} ${spaceTokens}
