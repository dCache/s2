; Parallel execution test (references to local/global variables)

ASSIGN v "v0"
   SYSTEM echo "${v} == v0 && ${0v} == v0"
      ASSIGN v "v1"
         SLEEP 1
            SYSTEM echo "${v} == v1 && ${0v} == v3"
            && ASSIGN 0v "v4"

   SLEEP 0 500000
      ASSIGN v "v2" 0v "v3"
         SYSTEM echo "${v} == v2 && ${0v} == v3"

&& SYSTEM echo "${v} == v4 && ${0v} == v4"
