TEST $MATCH{(?P<srmTest>.*).s2$ ${0}}
  #include ../include/Exhaust.s2

DEFUN srmTest authorizationID
   >0&&2 FUN ExhString $I{0} authorizationID ${authorizationID} : arg0
      ASSIGN c $EXPR{${c}+1}
      && ASSIGN script "eval=2 srmGetTransferProtocols $ENV{ENDPOINT} ${arg0}transferProtocols=(?P<transferProtocols>.*) returnStatus.explanation=(?P<returnStatus_explanation>.*) returnStatus.statusCode=(SRM_SUCCESS|SRM_DONE)"
      && FUN S2 ${script} $PRINTF{%02d ${c}}
      % Ignore errors
      || NOP

%%; main() %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%;
&& ASSIGN c 0
%&& <CODE FROM BASIC>
&& FUN srmTest $ENV{USER}
