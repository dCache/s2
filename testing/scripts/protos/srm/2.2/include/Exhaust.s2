%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
DEFUN S2 script i
   ASSIGN S2_S2 $ENV{S2_P}${i} S2_P $ENV{S2_P}${i} S2_D $ENV{S2_D}${i} S2_E $ENV{S2_E}${i} S2_L $ENV{S2_L}${i} S2_W $ENV{S2_W}${i} S2_E0 $ENV{S2_E0}${i} S2_E1 $ENV{S2_E1}${i} S2_E2 $ENV{S2_E2}${i}
#if 1
   && SYSTEM echo '${script}' | s2 --pp-out-file=${S2_P} --dbg-file=${S2_D} --err-file=${S2_E} --log-file=${S2_L} --warn-file=${S2_W} --e0-file=${S2_E0} --e1-file=${S2_E1} --e2-file=${S2_E2} --progress=0
#else
   && SYSTEM echo '${script}'
#endif
